{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Run npm dev",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "cwd": "${workspaceFolder}/frontend",
        "console": "integratedTerminal"
      },
      {
        "type": "debugpy",
        "request": "launch",
        "name": "Python: Webapi",
        "cwd": "${workspaceFolder}/backend",
        "program": "${workspaceFolder}/backend/main.py",
      }
    ]
  }
  